<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type='org.controlacceso.clientegwt.client.panel.BarraNavegacion.Style'>
    	.boton { margin: 10px 0px 0px 0px; width: 50px; border:1 1 1 1px solid #3E125F; cursor:pointer;}
		.mainPanel {border-bottom:1pt solid #3E125F;  width:100%; margin-top:10px;}
		.contenedorPaginas {margin: 10px 0px 0px 0px;}
		.pageAnchor {font-size:12px; margin: 0px 5px 0px 5px;}
		.pageSelectedAnchor {font-weight:bold; font-size:12px; text-decoration: underline; margin: 0px 5px 0px 5px;}
	</ui:style>
	<g:HorizontalPanel styleName="{style.mainPanel}">
		<g:Button ui:field="gotoPrevButton" styleName="{style.boton}">&lt;</g:Button>
		<g:cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE"  width="250px">
		    <g:HorizontalPanel ui:field="contenedorPaginas" styleName="{style.contenedorPaginas}"/>
		</g:cell>
		<g:Button ui:field="gotoNextButton" styleName="{style.boton}">&gt;</g:Button>
	</g:HorizontalPanel>
</ui:UiBinder> 