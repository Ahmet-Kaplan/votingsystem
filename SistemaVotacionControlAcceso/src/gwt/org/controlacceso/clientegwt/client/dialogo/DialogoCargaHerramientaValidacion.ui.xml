<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type='org.controlacceso.clientegwt.client.dialogo.DialogoCargaHerramientaValidacion.EditorStyle'>
		.indeterminatePanel {padding: 20px; width:350px; text-align: center;}
		.indeterminateLabel {font-size: 14px; margin:2px 3px 0px 0px; color:#47678e;
			width:100%; text-align: center; font-weight: bold;}
		.texto{}
		.indeterminateImage {display: block; margin-left: auto; margin-right: auto; margin-top:5px;}
   		.textPanel {margin: 10px; width:500px;}
		.textoChrome {}
		.boton { margin: 2px; cursor:pointer; float:right;}
	</ui:style>
	<ui:with field='res' type='org.controlacceso.clientegwt.client.Recursos'/>
	<ui:with field='constantes' type='org.controlacceso.clientegwt.client.Constantes'/>	
	<ui:image field='aceptar' src="../BotonAceptar.png"/>
	<g:DialogBox ui:field="dialogBox" autoHide="false" modal="true" 
	    glassEnabled="true" animationEnabled="true">
	    <g:VerticalPanel ui:field='mainPanel' styleName="{style.indeterminatePanel}">
   			 <g:VerticalPanel ui:field="textPanel" styleName="{style.textPanel}"/>
 			 <g:cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
     			 <g:VerticalPanel ui:field='indeterminatePanel' styleName="{style.indeterminatePanel}">
     	        	<g:Label ui:field='indeterminateLabel' styleName="{style.indeterminateLabel}"/>
	        		<g:Image resource='{res.indeterminate}' styleName="{style.indeterminateImage}"/>
			 	</g:VerticalPanel>
 			 </g:cell>
			<g:HorizontalPanel styleName="{style.boton}">
      			<g:PushButton  ui:field='aceptarButton'>
  						<g:upFace image='{aceptar}'/>
					</g:PushButton>
			</g:HorizontalPanel>
	    </g:VerticalPanel>
	</g:DialogBox>
</ui:UiBinder> 