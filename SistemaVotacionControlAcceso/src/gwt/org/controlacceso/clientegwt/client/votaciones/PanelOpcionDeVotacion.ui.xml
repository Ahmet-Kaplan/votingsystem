<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.mainPanel { height: 50px; padding:0px 0px 0px 40px; }
		.imgBorrarOpcion {margin: 7px 3px 0px 10px; cursor: pointer; cursor: hand;}
		.contenidoLabel {font-size: 26px; margin: 0px 10px 0px 10px;}
	</ui:style>
	<ui:with field='res' type='org.controlacceso.clientegwt.client.Recursos'/>
	<ui:image field='borrarOpcion' src="../BotonBorrarOpcion.png"/>
	<g:HorizontalPanel styleName="{style.mainPanel}">
	   	<g:cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:HorizontalPanel>
		        <!-- <g:Image resource='{res.cancel_16x16}' styleName="{style.imgBorrarOpcion}"  ui:field="borrarImage"/>-->
		  		<g:PushButton ui:field='borrarOpcionButton'>
					<g:upFace image='{borrarOpcion}'/>
				</g:PushButton>
				<g:Label styleName="{style.contenidoLabel}" ui:field="contenidoLabel"/>
			</g:HorizontalPanel>
		</g:cell>
	</g:HorizontalPanel>
</ui:UiBinder>