<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:svUtil="urn:import:org.controlacceso.clientegwt.client.util">
	<ui:style>
   		.textPanel {margin: 15px 20px 15px 20px; width:500px;}
		.boton { margin: 2px; cursor:pointer;}
		.buttonPanel { float: right; margin: 10px; margin-top: 15px;}
		.texto{font-size: 14px; font-weight:bold; }
	</ui:style>
	<ui:image field='anular' src="../BotonAnularSolicitudAcceso.png"/>
	<ui:image field='cerrar' src="../BotonCancelar.png"/>
	<ui:with field='constantes' type='org.controlacceso.clientegwt.client.Constantes'/>
	<g:DialogBox ui:field="dialogBox" autoHide="false" modal="true" glassEnabled="true" 
	    animationEnabled="true" text="{constantes.cancelAccessRequestCaption}">
		<g:VerticalPanel>
			<g:VerticalPanel styleName="{style.textPanel}">
			    <g:HTML ui:field='cancelAccessRequestMsg' styleName="{style.texto}" 
			        HTML='{constantes.cancelAccessRequestMsg}'/>
			    <g:HTML ui:field='accessRequestCanceledMsg' styleName="{style.texto}" 
			        HTML='{constantes.accessRequestCanceledMsg}'/>
			</g:VerticalPanel>
			<g:HorizontalPanel styleName="{style.buttonPanel}">
				<g:HorizontalPanel styleName="{style.boton}">
	      			<g:PushButton ui:field='cancelButton'>
   						<g:upFace image='{cerrar}'/>
 					</g:PushButton>
	      			<g:PushButton  ui:field='anularSolicitudButton'>
   						<g:upFace image='{anular}'/>
 					</g:PushButton>
				</g:HorizontalPanel>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:DialogBox>
	
</ui:UiBinder> 