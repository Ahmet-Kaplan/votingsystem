<?xml version="1.0"?>
<!-- Marker file indicating JSF 2.2 should be enabled in the application -->
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://xmlns.jcp.org/xml/ns/javaee
        http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

   <!-- Write your navigation rules here. You are encouraged to use CDI for  creating @Named managed beans. -->
    <application>
        <locale-config>
            <default-locale>es</default-locale>
            <supported-locale>es-ES</supported-locale>
            <supported-locale>en</supported-locale>
            <supported-locale>en-US</supported-locale>
        </locale-config>
        <resource-bundle>
            <base-name>org/votingsystem/web/controlcenter/messages</base-name>
            <var>msg</var>
        </resource-bundle>
    </application>

</faces-config>
