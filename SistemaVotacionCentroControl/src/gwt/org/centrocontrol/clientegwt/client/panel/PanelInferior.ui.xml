<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.mainPanel {border-top:1pt solid #3E125F; width: 968px; height: 42px;}	
		.herramientaValidacion {cursor: pointer; cursor: hand; font-size: 10px; font-weight:bold; 
			float:left; margin:0px 0px 0px 20px;}	
		.administracion {cursor: pointer; cursor: hand; font-size: 10px; font-weight:bold; 
			float:right; margin:0px 20px 0px 0px;}
	</ui:style>
	<ui:with field='constantes' type='org.centrocontrol.clientegwt.client.Constantes'/>
	<g:HorizontalPanel styleName="{style.mainPanel}">
   		<g:Anchor ui:field="herramientaValidacionAnchor" text='{constantes.herramientaValidacionLabel}'
			 styleName="{style.herramientaValidacion}"/>
		<g:HTML styleName="{style.administracion}" ui:field="administracionHTML"/>	
	</g:HorizontalPanel>
</ui:UiBinder>