<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
   		.textPanel {margin: 10px; width: 300px; height: 200px; }
		.buttonPanel { float: right; margin: 15px; }
	</ui:style>
	<ui:image field='aceptar' src="../BotonAceptar.png"/>
	<g:DialogBox ui:field="dialogBox" autoHide="false" modal="true" glassEnabled="true" animationEnabled="true">
		<g:caption><b>Error</b></g:caption>
		<g:VerticalPanel>
			<g:VerticalPanel ui:field="textPanel" styleName="{style.textPanel}">
			</g:VerticalPanel>
			<g:HorizontalPanel styleName="{style.buttonPanel}">
      			<g:PushButton  ui:field='aceptarButton'>
					<g:upFace image='{aceptar}'/>
				</g:PushButton>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:DialogBox>
</ui:UiBinder> 