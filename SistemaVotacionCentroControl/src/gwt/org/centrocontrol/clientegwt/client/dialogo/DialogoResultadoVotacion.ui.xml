<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:svUtil="urn:import:org.centrocontrol.clientegwt.client.util">
	<ui:style>
   		.textPanel {margin: 15px 20px 15px 20px; width:500px;}
		.boton { margin: 2px; cursor:pointer;}
		.buttonPanel { float: right; margin: 10px; margin-top: 15px;}
		.texto{}
		.image {margin:12px 0px 0px 15px;}
		.resultadoOperacion{margin: 10px 10px 15px 10px;font-size: 16px;}
		.popupLabel {cursor: pointer; cursor: hand; font-size: 14px; font-weight:bold; 
			position:relative; margin:12px 0px 0px 3px; color:#02227a;}	
	</ui:style>
	<ui:image field='aceptar' src="../BotonAceptar.png"/>
	<ui:with field='constantes' type='org.centrocontrol.clientegwt.client.Constantes'/>
	<ui:with field='res' type='org.centrocontrol.clientegwt.client.Recursos'/>
	<g:DialogBox ui:field="dialogBox" autoHide="false" modal="true" glassEnabled="true" 
	    animationEnabled="true" text="{constantes.resultadoVotacionOKCaption}">
		<g:VerticalPanel>
			<g:VerticalPanel ui:field="textPanel" styleName="{style.textPanel}">
   			    <g:HTML ui:field='resultadoOperacion' styleName="{style.resultadoOperacion}"/>
   				<g:HorizontalPanel>
			    	<g:Image resource='{res.view_detailed_16x16}' styleName="{style.image}"/>
					<svUtil:PopUpLabel ui:field="votoEnSistemaLabel"  text='{constantes.votoEnSistemaLabel}'
				    		styleName="{style.popupLabel}" />
				</g:HorizontalPanel>
   				<g:HorizontalPanel>			
   				    <g:Image resource='{res.filesave_16x16}' styleName="{style.image}"/>	
					<svUtil:PopUpLabel ui:field="guardarVotoLabel"  text='{constantes.guardarVotoLabel}'
				    		styleName="{style.popupLabel}" />
				</g:HorizontalPanel>				
   				<g:HorizontalPanel>	
   				    <g:Image resource='{res.signatureBad_16x16}' styleName="{style.image}"/>			
					<svUtil:PopUpLabel ui:field="anularVotoLabel"  text='{constantes.anularVotoLabel}'
				    		styleName="{style.popupLabel}" />
				</g:HorizontalPanel>				
			</g:VerticalPanel>
			<g:HorizontalPanel styleName="{style.buttonPanel}">
				<g:HorizontalPanel styleName="{style.boton}">
	      			<g:PushButton  ui:field='aceptarButton'>
   						<g:upFace image='{aceptar}'/>
 					</g:PushButton>
				</g:HorizontalPanel>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:DialogBox>
	
</ui:UiBinder> 